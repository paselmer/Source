structdef={structRawHeader,$
                           RecordNumber:0L,$
                           ExactTime:bytarr(27),$
                           NumChannels:4B,$
                           Resolution:30B,$
                           DetectorSequence:bytarr(9),$
                           NumberT_Probes:0B,$
                           NumberV_Probes:0B,$
                           Reserved:bytarr(20)};7 head items,1 reserved;64bytes
structdef={structEngineeringData,$
                                 LaserEnergyMonitors:lonarr(3),$
                                 TemperatureMeasurements:lonarr(12),$
                                 VoltageMeasurements:lonarr(3),$
                                 Pressure:0L,$
                                 Reserved:lonarr(16)};4 engineering items, 1reserved;140bytes
